rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Apenas permitir que utilizadores autenticados leiam e escrevam na sua pr√≥pria pasta de imagens de perfil.
    match /profile-pictures/{userId} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }
  }
}
